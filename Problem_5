#include <stdio.h>

#define MAX 50
int cq[MAX], front = -1, rear = -1;

void cEnq(int x){
    if((front==0 && rear==MAX-1) || (rear+1)%MAX==front)
        printf("Overflow\n");
    else{
        if(front==-1) front = rear = 0;
        else rear = (rear+1)%MAX;
        cq[rear] = x;
    }
}

void cDeq(){
    if(front==-1) printf("Underflow\n");
    else{
        printf("Deleted: %d\n", cq[front]);
        if(front==rear) front = rear = -1;
        else front = (front+1)%MAX;
    }
}

void cDisp(){
    if(front==-1) printf("Empty\n");
    else{
        int i = front;
        while(1){
            printf("%d ", cq[i]);
            if(i==rear) break;
            i = (i+1)%MAX;
        }
        printf("\n");
    }
}

int main(){
    int ch, val;
    do{
        printf("\n1.Enq 2.Deq 3.Display 4.Exit: ");
        scanf("%d",&ch);

        if(ch==1){
            printf("Enter value: ");
            scanf("%d",&val);
            cEnq(val);
        }
        else if(ch==2) cDeq();
        else if(ch==3) cDisp();

    }while(ch!=4);
    return 0;
}
