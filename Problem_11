#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

struct node *head1 = NULL, *head2 = NULL;

// ---- INSERT AT END ----
void insert(struct node **head, int x) {
    struct node *n = malloc(sizeof(struct node));
    n->data = x;
    n->next = NULL;

    if (*head == NULL)
        *head = n;
    else {
        struct node *t = *head;
        while (t->next != NULL)
            t = t->next;
        t->next = n;
    }
}

// ---- DISPLAY LIST ----
void display(struct node *head) {
    if (!head) { printf("Empty\n"); return; }
    while (head) {
        printf("%d ", head->data);
        head = head->next;
    }
    printf("\n");
}

// ---- CONCATENATE LIST2 AT END OF LIST1 ----
void concatenate() {
    if (head1 == NULL) {
        head1 = head2;
        return;
    }
    struct node *t = head1;
    while (t->next != NULL)
        t = t->next;
    t->next = head2;
}

int main() {
    int n1, n2, x;

    printf("How many nodes in List 1? ");
    scanf("%d", &n1);
    for (int i = 0; i < n1; i++) {
        scanf("%d", &x);
        insert(&head1, x);
    }

    printf("How many nodes in List 2? ");
    scanf("%d", &n2);
    for (int i = 0; i < n2; i++) {
        scanf("%d", &x);
        insert(&head2, x);
    }

    printf("\nList 1: ");
    display(head1);

    printf("List 2: ");
    display(head2);

    concatenate();

    printf("\nConcatenated List: ");
    display(head1);

    return 0;
}
